#include <stdio.h>
#include <stdlib.h>
int binary_search(int* a,int l,int h,int x)
{
    int mid = l + (h-l)/2;
    if(h>=l){
        if(a[mid]==x)
            return mid;
        else if(a[mid]>x)
            return binary_search(a,l,mid-1,x);
        else
            return binary_search(a,mid+1,h,x);
    }
    else
        return -1;
}
int main()
{
    int n,no_to_find;
    printf("Enter the value of n : ");
    scanf("%d",&n);
    int a[100];
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    printf("Enter the number to find : ");
    scanf("%d",&no_to_find);
    int result = binary_search(a,0,n-1,no_to_find);
    if( result == -1)
    {
        printf("Sasuke %d",no_to_find);
    }
    else
    {
        printf("Naruto %d",result+1);
    }
    
}
